{
  "name": "kepler-server",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node dist/app.js",
    "dev": "nodemon --files src/app.ts",
    "build": "tsc",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "clean": "echo \"Cleaning build and dependencies\" && rimraf ./dist ./node_modules && echo \"Done\"",
    "reset": "echo \"Cleaning dependencies and reinstall\" && rimraf ./node_modules && npm install && echo \"Done\"",
    "posti": "if [ \"$NODE_ENV\" != \"production\" ] && [ \"$USER\" == \"hungtran\" ]; then husky install; fi",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.{js,ts}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/hapi__joi": "^17.1.15",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
